import gleeunit
import gleeunit/should
import aoc2022/day6

pub fn main() {
  gleeunit.main()
}

pub fn day6part1_example_test() {
  sample
  |> day6.start_of_packet_marker()
  |> should.equal(7)

  sample2
  |> day6.start_of_packet_marker()
  |> should.equal(5)

  sample3
  |> day6.start_of_packet_marker()
  |> should.equal(6)

  sample4
  |> day6.start_of_packet_marker()
  |> should.equal(10)

  sample5
  |> day6.start_of_packet_marker()
  |> should.equal(11)
}

pub fn day6part1_data_test() {
  data
  |> day6.start_of_packet_marker()
  |> should.equal(1093)
}

pub fn day6part2_example_test() {
  sample
  |> day6.start_of_message_marker()
  |> should.equal(19)

  sample2
  |> day6.start_of_message_marker()
  |> should.equal(23)

  sample3
  |> day6.start_of_message_marker()
  |> should.equal(23)

  sample4
  |> day6.start_of_message_marker()
  |> should.equal(29)

  sample5
  |> day6.start_of_message_marker()
  |> should.equal(26)
}

pub fn day6part2_data_test() {
  data
  |> day6.start_of_message_marker()
  |> should.equal(3534)
}

